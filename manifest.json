{
  "manifest_version": 2,
  "name": "Gibbon Tabs",
  "description": "A Chrome extension to help you manage your tabs effectively.",
  "version": "1.1.0",
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
