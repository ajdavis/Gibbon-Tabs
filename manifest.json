{
  "manifest_version": 2,
  "name": "Gibbon Tabs",
  "description": "A Chrome tab manager to help you navigate your tab jungle with ease.",
  "version": "1.0",
  "background": {
    "scripts": ["js/background.js"]
  },
  "commands": {
    "show_tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+E"
      },
      "description": "Activates a popup which lets you quickly manage your tabs."
    }
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
